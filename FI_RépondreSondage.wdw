  procedures :
   -
     name : MiseAJourInterface
     procedure_id : 1208661598771467288
     type_code : 14
     code : |1-
      // Résumé : Cette procédure sert à mettre à jour l'interface du plan Question
      // Syntaxe :
      //MiseAJourInterface ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      Procédure MiseAJourInterface(LOCAL stQuestionEnCours est un TypeQuestion, LOCAL nNbreQuestions est un entier)
      
      sRéponse est une chaine
      i est un entier = 1
      
      
      
      LIB_NUMQUESTION = chaineconstruit(<§$0004§>,stQuestionEnCours:nNumQuestion, nNbreQuestions)
      
      // Mise à jour du libellé de la question
      LibQuestion = stQuestionEnCours:sLibellé
      
      si stQuestionEnCours:nMinRéponses = stQuestionEnCours:nMaxRéponses ALORS
      	si stQuestionEnCours:nMaxRéponses > 1 alors
      		// Mise à jour du libellé du nombre de réponses max
      		LIB_REPONSESMAX = chaineconstruit(<§$0005§>,stQuestionEnCours:nMaxRéponses)
      	sinon
      		// Mise à jour du libellé du nombre de réponses max
      		LIB_REPONSESMAX = ChaîneConstruit(<§$0009§>,stQuestionEnCours:nMaxRéponses)
      	fin
      sinon
      	LIB_REPONSESMAX = 	chaineconstruit(<§$0006§>,stQuestionEnCours:nMinRéponses,stQuestionEnCours:nMaxRéponses)
      FIN
      
      
      
      // Mise à jour des valeurs des options de l'interrupteur
      POUR TOUTE CHAINE sRéponse DE stQuestionEnCours:sListeRéponse SEPAREE PAR rc
      	nIndice est un entier = zmlAjouteLigne(ZM_ChoixRéponses)
      	ZM_ChoixRéponses[nIndice].LIB_Réponse = sRéponse
      FIN
      		
      // Si une réponse a été précédemment sélectionnée pour cette question
      si stQuestionEnCours.sRéponse <> "" ALORS
      	// Applique la réponse précédente
      	pour tout element nReponse, nIndice de stQuestionEnCours.tRéponseSélectionnées
      		ZM_ChoixRéponses[nIndice].IMG_Checkmark..Visible = nReponse
      		// Si l'indice n'est plus valide, stoppe le parcours
      		si nIndice = ZM_ChoixRéponses..Occurrence ALORS
      			sortir
      		FIN
      	FIN
      FIN
     type : 458752
   -
     name : SauveRéponses
     procedure_id : 1208661663195992172
     type_code : 14
     code : |1-
      // Résumé : Sauvegarde en mémoire, dans un tableau, des réponses au sondage, 
      //pour les enregistrer toutes d'un coup une fois que toutes les réponses au sondage auront été validées
      // Syntaxe :
      // SauveRéponses ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      Procédure SauveRéponses()
      
      sRéponse est une chaine
      bChoisi est un booleen
      
      pour i = 1 _a_ ZM_ChoixRéponses..Occurrence
      	bChoisi = ZM_ChoixRéponses[i].IMG_Checkmark..Visible
      	
      	// On sauvegarde dans le tableau
      	LaQuestion:tRéponseSélectionnées[i] = bChoisi
      	
      	si bChoisi = vrai alors 
      		// On met à jour la réponse
      		sRéponse += [TAB] + ZM_ChoixRéponses[i].LIB_Réponse
      	FIN
      FIN
      
      
      
      // Mise à jour du tableau
      LaQuestion:sRéponse = sRéponse
      
      si MaFenêtre..nom = "FEN_FicheSondage" ALORS
      	 FEN_FicheSondage.gtabQuestions[gnNumQuestion] = LaQuestion
      FIN
     type : 458752
