   -
     internal_properties : CAAAAAgAAACpi7z3B7xtFtHq56gET75/meterDa319Mx2PPe6GUp4RsOQOC92r3h8olf1nRKU/wp3CgUJmqdYblIs2TZ55Pvxq7fAnJ/n/HD3Q2b56G1PayWb7brMtHgE9Pg+xhw0HuKlooQz4Xw7OL0FU4=
     code : |1+
      
      
      selon mafenêtrefille..nom
      
      
      	CAS "FEN_MENUENREG" :
      		
      		selon MaFenêtreFille..ValeurRenvoyée
      		
      			cas CST_AJOUTER_QUESTION_SONDAGE :
      				si VérifSaisies() = vrai  ALORS
      					
      					AjoutModifQuestion()
      					
      				FIN
      				
      				
      			cas CST_TERMINER_CREATION_SONDAGE :
      				
      				
      				si VérifSaisies() = vrai alors
      					
      					AjoutModifQuestion()
      					
      				fin				
      				RemplitFicheModif()
      				RemplitZRQuestions()	
      				
      				gnModeSondage = CST_MODIFIERSONDAGE
      				
      				mafenêtrE..plan = 4
      				ChangementPlan(4)
      				
      				
      		FIN
      		
      		
      	cas "FEN_POPUP_OUVRIR" :
      		
      		IDSONDAGE_ENCOURS = MaFenêtreFille..ValeurRenvoyée 
      		
      		// Charge le sondage
      		HLitRecherchePremier(Sondage, IDSondage, IDSONDAGE_ENCOURS)
      		SI HTrouve(Sondage) = faux ALORS
      			
      			mafenêtre..Plan = 5
      			ChangementPlan(5)
      			
      		sinon
      			
      			SELON gnModeSondage
      			
      				CAS CST_OUVRIR :
      					
      					FenCoulissanteVisible(fcGauche, Faux)
      					
      					mafenêtre..Titre = ChaîneConstruit("Résultats du sondage '%1'",Sondage.NomSondage)
      					
      					NUMQUESTION_ENCOURS = 1
      					
      					// On remplit le tableau
      					RemplitTableau()
      					
      					// Mise à jour des informations
      					MAJRésultat()				
      					
      					MaFenêtre..Plan = 2
      					ChangementPlan(2)
      					
      				CAS CST_MODIFIERSONDAGE :											
      					
      					FenCoulissanteVisible(fcGauche, Faux)
      						
      					NUMQUESTION_ENCOURS = 1
      					
      					//On cherche la 1ère question du sondage 
      					//Et si on ne trouve pas, on passe en mode création
      					HLitRecherchePremier(Question, NumQuestion,NUMQUESTION_ENCOURS)
      					
      					SI PAS HTrouve ALORS
      						gnModeSondage = CST_CREERSONDAGE 
      					FIN
      					
      					RemplitFicheModif()
      					RemplitZRQuestions()
      					
      					MaFenêtre..Plan = 4
      					ChangementPlan(4)
      					
      					
      				cas CST_CONSULTERSONDAGE :
      					FenCoulissanteVisible(fcGauche, Faux)			
      					
      					MaFenêtre..Titre = ChaîneConstruit("Consultation du sondage '%1'",Sondage.NomSondage)
      					
      					tabQuestions 			est un tableau dynamique de TypeQuestion	// Tableau contenant toutes les questions
      					
      					HLitRecherchePremier(Sondage,IDSondage,IDSONDAGE_ENCOURS)
      					SI HTrouve(Sondage) ALORS
      						tabQuestions = RemplitTableauQuestions(IDSONDAGE_ENCOURS, Sondage.NomSondage)
      					FIN
      					
      					POUR i = 1 _a_ tabQuestions..Occurrence
      						FIListeAjoute(CFI_ConsultSondage,FI_RépondreSondage, csvisu, tabQuestions[i], i, tabQuestions..Occurrence)
      					FIN
      										
      					MaFenêtre..Plan = 1
      					ChangementPlan(1)	
      					
      				CAS CST_SUPPRIMERSONDAGE :
      					
      					HSupprime(Sondage)
      					gbSondageModifié = vrai
      					FenCoulissanteVisible(fcGauche, Faux)			
      					
      					
      			FIN
      			
      		FIN
      		
      		
      	cas "FEN_POPUP_ENREGISTRER"
      		
      		IDSONDAGE_ENCOURS = MaFenêtreFille..ValeurRenvoyée 
      		
      		// Charge le sondage
      		HLitRecherchePremier(Sondage, IDSondage, IDSONDAGE_ENCOURS)
      		SI HTrouve(Sondage) ALORS
      			
      			mafenêtre..titre = ChaîneConstruit("Création du sondage '%1'", Sondage.NomSondage)
      			
      			gnModeSondage = CST_CREERSONDAGE
      			gnModeQuestion = CST_CREERQUESTION
      			
      			NUMQUESTION_ENCOURS = 1
      			
      			// Mise à jour des informations
      			LIB_NuméroQuestionEncours = ChaîneConstruit(<§$0002§>,NUMQUESTION_ENCOURS)
      			RéinitFicheQuestion()
      			
      			FenCoulissanteVisible(fcGauche, Faux)
      			
      			MaFenêtre..Plan = 3 
      			ChangementPlan(3)
      			
      			HLitDernier(Sondage)
      			
      			
      		fin
      		
      		
      		
      FIN
