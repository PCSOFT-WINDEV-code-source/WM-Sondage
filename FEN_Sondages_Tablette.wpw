              code : |1+
               //Si on n'est pas sur la dernière ligne, qu'elle est vide et qu'il y a plus d'1 ligne dans la ZR, on supprime
               //Le but c'est de toujours avoir au maximum 1 ligne vide 
               SI ZM_Réponses..Occurrence > 1  _et_ ZM_Réponses <> ZM_Réponses..Occurrence _et_ moimeme = "" ALORS
               	
               	//Suppression de la ligne en cours
               	zmlSupprime(ZM_Réponses,ZM_Réponses)
               	
               	SI ZM_Réponses..Occurrence = 1  ALORS
               		ZM_Réponses[1].BTN_Supprimer..Visible = Faux
               	FIN
               	
               FIN
              type : 16
            -
