              enabled : false
            -
              type : 278
              enabled : false
            -
              code : |1+
               
               SI moimeme = "" ALORS
               	
               	bEtaitVide = Vrai 
               	//On ne propose plus la suppression de cette ligne puisqu'elle est devenue vide 
                   BTN_Supprimer..Visible = Faux
               	
               SINON
               	
               	//Si la ligne n'est plus vide alors qu'elle l'était
               	SI bEtaitVide ALORS	
               		
               		//Nombre de réponses limité à 5 dans cet exemple
               		si ZM_Réponses..Occurrence < csNbRéponsesMax ALORS			
               					
               			//On propose maintenant la suppression de la réponse
               			BTN_Supprimer..visible = Vrai 
               			
               			//Et on propose la création d'une nouvelle réponse possible
               			zmlAjouteLigne(ZM_Réponses)
               		fin
               		
               		bEtaitVide = Faux
               	FIN
               FIN
              type : 17
        -
          name : BTN_Supprimer
          identifier : 0x234e8fd623bca77b
          internal_properties : 
          properties :
           x : 573
           y : 6
