   -
     name : RéinitFicheQuestion
     procedure_id : 1391207484239357819
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Nouvelle_Procédure ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      PROCEDURE RéinitFicheQuestion()
      
      //Efface tout pour créer une nouvelle question
      HRAZ(Question)
      
      //Ré-initialisation des champs
      SAI_LibelléQuestion = ""
      SEL_TypeRéponse = 1
      ZM_Réponses..Visible = Vrai
      
      zmlSupprimeTout(ZM_Réponses)
      zmlAjouteLigne(ZM_Réponses)
      
      SAI_NbreRéponsesMin = 1
      SAI_NbreRéponsesMax = 1 
      
     type : 458752
   -
     name : VérifSaisies
     procedure_id : 1391207484239423355
     type_code : 14
     code : |1-
      // Résumé : Vérification des saisies pour l'ajout d'une nouvelle question
      // Syntaxe :
      //[ <Résultat> = ] VérifSaisies ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	booléen : Vrai si tout s'est bien passé, Faux sinon
      
      PROCEDURE VérifSaisies()
      
      // On vérifie les valeurs saisies
      SI SAI_LibelléQuestion = "" ALORS
      	Erreur(<§$000a§>)
      	renvoyer faux
      FIN
      
      
      nNbreRéponsesSaisies est un entier
      POUR i = 1 _à_ ZM_Réponses..occurrence 
      	SI ZM_Réponses[i].SAI_Réponse <> "" ALORS
      		nNbreRéponsesSaisies++
      	FIN
      FIN 
      
      SI nNbreRéponsesSaisies = 0 ALORS
      	Erreur(<§$000b§>)
      	RENVOYER Faux
      FIN
      //Blindage, mais ne doit pas arrivé, étant donné le code de "à chaque modification" des champs de saisie "réponse minimum" et "réponse maximum"
      SI SAI_NbreRéponsesMax < SAI_NbreRéponsesMin ALORS
      	Erreur(<§$000d§>)
      	RENVOYER Faux
      FIN
      
      SI nNbreRéponsesSaisies < SAI_NbreRéponsesMin  ALORS
      	Erreur(ChaîneConstruit(<§$000c§>, SAI_NbreRéponsesMin, nNbreRéponsesSaisies))
      	RENVOYER Faux
      FIN
      
      SI SAI_NbreRéponsesMax > nNbreRéponsesSaisies ALORS
      	Erreur(ChaîneConstruit(<§$000f§>, SAI_NbreRéponsesMax, nNbreRéponsesSaisies))
      	RENVOYER Faux
      FIN
      
      renvoyer vrai
     type : 458752
   -
     name : AjoutModifQuestion
     procedure_id : 1391207484239488891
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //AjoutModifQuestion ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      PROCEDURE AjoutModifQuestion()
      
      // Gestion de l'ajout ou de la modification de la question
      SELON gnModeQuestion
      
      	CAS CST_CREERQUESTION :
      		
      		// Mise à jour du Contexte HyperFileSQL
      		Question.IDSondage = IDSONDAGE_ENCOURS
      		Question.LibelléQuestions = SAI_LibelléQuestion
      		Question.MinRéponse = SAI_NbreRéponsesMin
      		Question.MaxRéponses = SAI_NbreRéponsesMax
      		Question.NumQuestion = NUMQUESTION_ENCOURS
      		Question.TypeRéponse = SEL_TypeRéponse
      		
      		POUR i = 1 _a_ ZM_Réponses..Occurrence
      			{"Question.Réponse"+i,indRubrique} = ZM_Réponses[i].SAI_Réponse
      		FIN
      		
      		// Ajoute la question
      		HAjoute(Question)
      		
      		Sondage.NbQuestions = Sondage.NbQuestions + 1
      		HModifie(Sondage)
      		gbSondageModifié = vrai
      		
      	CAS CST_MODIFIERQUESTION :
      		
      		//On se positionne sur la question à modifier
      		
      		HLitRecherchePremier(Question,IDQuestion, IDQUESTION_ENCOURS)
      		SI HTrouve() = Vrai ALORS 
      			Question.LibelléQuestions = SAI_LibelléQuestion
      			Question.TypeRéponse = SEL_TypeRéponse		
      			Question.MinRéponse = SAI_NbreRéponsesMin
      			Question.MaxRéponses = SAI_NbreRéponsesMax
      			
      			POUR i = 1 A 5
      				{"Question.Réponse"+i,indRubrique} = ""
      			FIN
      			
      			POUR i = 1 _a_ ZM_Réponses..Occurrence
      				{"Question.Réponse"+i,indRubrique} = ZM_Réponses[i].SAI_Réponse
      			FIN
      			
      			HModifie(Question)
      		FIN
      FIN
      
      NUMQUESTION_ENCOURS++
      LIB_NuméroQuestionEncours = ChaîneConstruit(<§$0002§>,NUMQUESTION_ENCOURS)
      
      gnModeQuestion = CST_CREERQUESTION
      
      RéinitFicheQuestion()
      
     type : 458752
   -
     name : ChangementPlan
     procedure_id : 1391564490468821409
     type_code : 14
     code : |1+
      // Résumé : Modification de l'IHM en cas de changement de plan
      // Syntaxe :
      //ChangementPlan (<nNouveauPlan> est entier)
      //
      // Paramètres :
      //	nNouveauPlan (entier) : Numéro du plan à affiche
      // Valeur de retour :
      // 	Aucune
      
      PROCEDURE ChangementPlan(nNouveauPlan est un entier)
      
      selon nNouveauPlan
      
      	cas 3 :
      		OPT_Annuler..Visible = vrai 
      		OPT_OK..Visible = Vrai 
      		
      		OPT_Ajouter..Visible = faux 
      		
      	cas 4 : 
      		OPT_Annuler..Visible = faux 
      		OPT_OK..Visible = faux 
      		
      		OPT_Ajouter..Visible = vrai 
      		
      	AUTRE CAS
      		OPT_Annuler..Visible = Faux 
      		OPT_OK..Visible = Faux 
      		
      		OPT_Ajouter..Visible = Faux 
      FIN
      
      
