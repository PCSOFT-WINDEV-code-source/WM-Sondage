  procedures :
   -
     name : CreeGraphe
     procedure_id : 1391207484239030139
     type_code : 14
     code : |1+
      // Résumé : Création du graphe des résultats du sondage pour une réponse donnée
      // Syntaxe :
      // CreeGraphe ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      Procédure CreeGraphe()
      
      sRéponse est une chaine
      
      
      grSupprimeSérie(GRF_Graphe,1)
      
      // On ajoute les données
      pour i = 1 a csNbRéponsesMax
      	
      	sRéponse = extraitchaine(TAB_REPONSES[NUMQUESTION_ENCOURS]:sListeRéponses,i,rc)
      	si sRéponse <> eot et sRéponse <> "" alors
      		grAjouteDonnée(GRF_Graphe,1,i,  TAB_REPONSES[NUMQUESTION_ENCOURS]:tabRéponse[i])
      		grEtiquetteCatégorie(GRF_Graphe,i,sRéponse)
      		grCouleurserie(GRF_Graphe,i, tsl(7,i*20,100-i*10))
      	fin
      	
      FIN
      
      // Dessin du graphe
      grDessine(GRF_Graphe)
     type : 458752
   -
     name : MAJRésultat
     procedure_id : 1391207484239095675
     type_code : 14
     code : |1+
      // Syntaxe : Affichage du résultat pour une question donnée du sondage
      //MAJRésultat ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      Procédure MAJRésultat()
      
      // Contrôle l'indice de la question
      SI NUMQUESTION_ENCOURS <= 0 alors NUMQUESTION_ENCOURS = 1
      SI NUMQUESTION_ENCOURS > TAB_REPONSES..Occurrence alors NUMQUESTION_ENCOURS = TAB_REPONSES..Occurrence
      
      // Affiche les flèches
      SELON NUMQUESTION_ENCOURS 
      
      	CAS 1 
      		BTN_Précédent..Visible = Faux
      		BTN_Suivant..Visible = Vrai
      		
      	CAS Dimension(TAB_REPONSES) 
      		BTN_Précédent..Visible = Vrai
      		BTN_Suivant..Visible = Faux
      		
      	AUTRES CAS
      		BTN_Précédent..Visible = Vrai
      		BTN_Suivant..Visible = Vrai
      		
      FIN
      
      
      si TAB_REPONSES..Vide = faux ALORS
      	
      	// Mise à jour des informations
      	LIB_NuméroQuestion = chaineconstruit(<§$0002§>,NUMQUESTION_ENCOURS)
      	LIB_LibelléQuestion =  ESP + ESP + TAB_REPONSES[NUMQUESTION_ENCOURS]:slibelleQuestion
      	LIB_REPONSES = TAB_REPONSES[NUMQUESTION_ENCOURS]:sListeRéponses
      	
      	// Création du graphe
      	si TAB_REPONSES[NUMQUESTION_ENCOURS]:bRéponses = vrai ALORS
      		LIB_PASREPONSES..visible = faux
      		GRF_Graphe..Visible = vrai
      		CreeGraphe()	
      	sinon
      		GRF_Graphe..Visible = faux
      		LIB_PASREPONSES..visible = Vrai
      	FIN
      	
      FIN
