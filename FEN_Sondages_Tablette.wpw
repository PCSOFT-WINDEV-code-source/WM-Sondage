   -
     name : RemplitTableau
     procedure_id : 1391207484239226747
     type_code : 14
     code : |1-
      // Résumé : Remplit le tableau des résultats du sondage
      // Syntaxe :
      // RemplitTableau ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      Procédure RemplitTableau()
      
      Réponse est un STtypeRéponse
      sRéponse est une chaine
      sRéponseDonnée est une chaine
      i est un entier = 0
      j est un entier = 1
      
      // Supprime tout ce qu'il y a dans le tableau
      TableauSupprimetout(TAB_REPONSES)
      
      pour TOUT Question AVEC IDSondage = IDSONDAGE_ENCOURS
      	
      	// Remplissage de la structure de la question correspondante
      	Réponse:nNumQuestion = question.NumQuestion 
      	Réponse:sLibelléQuestion = question.LibelléQuestions
      	
      	// Remplissage de la liste des réponses possibles
      	Réponse:sListeRéponses = "- " + question.Réponse1 + rc
      	
      	pour i = 2 a csNbRéponsesMax
      		// Récupération de la réponse
      		sRéponse = {"Question.Réponse" + i,indRubrique}
      		// Si la réponse est différente de ""
      		si sRéponse <>"" ALORS
      			// On l'ajoute dans la liste
      			Réponse:sListeRéponses += "- " + sRéponse + rc		
      		FIN
      	FIN
      	
      	// Mise à jour des paramètres de la requête
      	REQ_Résultat.nIDQuestion = question.IDQuestion
      	REQ_Résultat.nIDSondage = IDSONDAGE_ENCOURS
      	
      	// Exécution de la requête
      	HExécuteRequête(REQ_Résultat)
      	
      	sRéponse =""
      	// On lit le premier enregistrement trouvé
      	hlitpremier(REQ_Résultat)
      	// Vérification du nombre de réponses
      	si hnbenr(REQ_Résultat) = 0 ALORS
      		Réponse:bRéponses = faux
      	sinon
      		Réponse:bRéponses = vrai
      		
      		tantque pas HEnDehors(REQ_Résultat)
      			
      			// La réponse donnée est le cas d'un sélecteur
      			si question.MaxRéponses = 1 ALORS
      				
      				// Si la réponse sauvegardée est différente de celle en cours, 
      				si sRéponse <> REQ_Résultat.Réponse ALORs	
      					i=1
      					
      					// on sauvegarde la nouvelle réponse
      					sRéponse = REQ_Résultat.Réponse
      					
      					// On extrait la réponse à tester dans la liste des réponses possibles
      					sRéponseDonnée = extraitchaine(Réponse:sListeRéponses,i,rc)
      					// on enlève le "- "
      					sRéponseDonnée = sRéponseDonnée[[3 a ]]
      					
      					// Tant qu'on est pas au max des réponses possibles
      					tantque i<=csNbRéponsesMax
      						// Si la réponse testée correspond à la réponse en cours
      						si sRéponse = sRéponseDonnée ALORS
      							// On sauvegarde le résultat
      							Réponse:tabRéponse[i]= REQ_Résultat.NbRéponse
      							// on sort de la boucle
      							sortir
      						sinon
      							// Incrémentation du compteur
      							i++
      							// Lit la réponse possible suivante
      							sRéponseDonnée = extraitchaine(Réponse:sListeRéponses,i,rc)
      							// On enlève le "- "
      							sRéponseDonnée = sRéponseDonnée[[3 a ]]
      						FIN
      					FIN	
      				FIN
      			sinon
      				
      				// On est dans le cas d'un interrupteur
      				pour i = 1 _a_ Question.MaxRéponses
      					sRéponse = extraitchaine(REQ_Résultat.Réponse,i)
      					
      					pour j = 1 à csNbRéponsesMax 
      						sRéponseDonnée = extraitchaine(Réponse:sListeRéponses,j,rc)	
      						si sRéponseDonnée[[3 a]] = sRéponse ALORS
      							Réponse:tabRéponse[j] += REQ_Résultat.NbRéponse
      						FIN
      					FIN
      					
      				FIN
      			fin
      			hlitsuivant(REQ_Résultat)
      		fin
      	FIN
      	
      	tableauajoute(TAB_REPONSES,Réponse)
      	
      	
      	// On remet le tableau à 0
      	pour i = 1 a csNbRéponsesMax
      		Réponse:tabRéponse[i] = 0
      	FIN
      	
      FIN
      // On trie le tableau
      tableautrie(tab_Réponses,ttMembre,"+nNumQuestion")
     type : 458752
   -
     name : RemplitZRQuestions
     procedure_id : 1391207484239292283
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Nouvelle_Procédure ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE RemplitZRQuestions()
      
      nNouvelleHauteur est un entier
      
      ZoneRépétéeSupprimeTout(ZR_Questions)
      
      // Recherche des questions du sondage
      POUR TOUT Question AVEC IDSondage = IDSONDAGE_ENCOURS 
      	
      	// Ajout dans la table de toutes les questions
      	ZoneRépétéeAjouteLigne(ZR_Questions,Question.IDQuestion,Question.LibelléQuestions,Question.NumQuestion,Question.TypeRéponse,Question.MinRéponse, Question.MaxRéponses, 
      	Question.Réponse1, Question.Réponse2, Question.Réponse3, Question.Réponse4, Question.Réponse5)
      	
      	ZoneRépétéeTrie("ATT_NumQuestion")
      	
      FIN
      
      si ZR_Questions..Occurrence = 0 ALORS
      	ZR_Questions..Visible = Faux
      sinon
      	ZR_Questions..Visible = vrai 
      	
      	
      	nNouvelleHauteur = ZR_Questions..Occurrence * ZR_Questions..HauteurInitiale
      	
      	//Redimensionnement de la ZR, en fonction du nombre de questions
      	ZR_Questions..Hauteur = nNouvelleHauteur
      	
      FIN
      
      
      
