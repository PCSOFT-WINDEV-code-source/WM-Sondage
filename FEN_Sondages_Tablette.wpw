           p_codes :
            -
              code : |1+
               //Traitement différent de si on avait atteint le nombre maximal de réponses possibles, ou non 
               si ZM_Réponses..Occurrence = csNbRéponsesMax ALORS
               	
               	//Suppression de la ligne en cours
               	zmlSupprime(ZM_Réponses,ZM_Réponses)
               	
               	//On vérifie qu'on ait toujours quand même une ligne vide, si non on en ajoute une 
               	SI ZM_Réponses[ZM_Réponses..Occurrence].SAI_Réponse <> "" ALORS
               		zmlAjouteLigne(ZM_Réponses)
               		
               		//Et on rend l'avant dernière ligne supprimable
               		ZM_Réponses[ZM_Réponses..Occurrence - 1].BTN_Supprimer..Visible = vrai
               	fin
               	
               sinon
               	
               	//Suppression de la ligne en cours
               	zmlSupprime(ZM_Réponses,ZM_Réponses)
               
               	SI ZM_Réponses..Occurrence = 1 ALORS
               		ZM_Réponses[1].BTN_Supprimer..Visible = Faux
               	FIN
               	
               FIN
               
               
               
