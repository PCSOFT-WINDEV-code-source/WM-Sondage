     p_codes :
      -
        code : |1+
         
         //************** Vérification de la saisie
         
         // Vérifie si le nom n'est pas vide
         si SAI_NomSondage = "" ALORS
         	erreur("Veuillez indiquer un nom")
         	DonneFocus(SAI_NomSondage)
         	retour
         FIN
         
         // Vérifie si le nom est déjà utilisé
         HLitRecherchePremier(sondage,NomSondage, SAI_NomSondage)
         si htrouve(Sondage) ALORS
         	erreur("Ce nom est déjà utilisé")
         	DonneFocus(SAI_NomSondage)
         	retour
         FIN
         
         //Enregistrement du sondage dans la base de données
         HRAZ(Sondage)
         Sondage.CrééLe	 	= DateDuJour
         Sondage.CrééPar	= SAI_Auteur
         Sondage.NomSondage			= SAI_NomSondage
         HAjoute(Sondage)
         gbSondageModifié = vrai
         
         mafenêtre..ValeurRenvoyée = sondage.IDSondage
         
         ferme()
        type : 18
      -
        type : 161
        enabled : false
